\pard\tx220\tx720\pardeftab720\li720\fi-720\sa240\partightenfactor0
\ls87\ilvl0
\f4\fs32 \cf2 \cb3 \kerning1\expnd0\expndtw0 \outl0\strokewidth0 {\listtext	\uc0\u8226 	}\expnd0\expndtw0\kerning0
\outl0\strokewidth0 \strokec2 A UTC\'a0
\f5\fs28 \cf8 \cb9 \strokec8 timestamp
\f4\fs32 \cf2 \cb1 \strokec2 \
\ls87\ilvl0\cb3 \kerning1\expnd0\expndtw0 \outl0\strokewidth0 {\listtext	\uc0\u8226 	}\expnd0\expndtw0\kerning0
\outl0\strokewidth0 \strokec2 The\'a0
